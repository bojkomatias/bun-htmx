    "elysia": "latest",
    "tailwind-merge": "^1.14.0"
  },
  "devDependencies": {
    "bun-types": "latest",
    "concurrently": "^8.2.0",
    "drizzle-kit": "^0.19.13",
    "install": "^0.13.0",
    "prettier": "^3.0.3",
